

function addExclamation(s) {
  let newString = this.newString;
  let exclam = this.exlclam;
  this.exclam = "!";
  this.newString = s.concat(this.exclam);
  return this.newString;
  }

console.log(addExclamation('hello world'), '<-- should be "hello world!"');

function addExclamation(s) {
 s = s + '!';
 return s;
}


console.log(addExclamation('hello world'), '<-- should be "hello world!"');
